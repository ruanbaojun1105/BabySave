<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    >
    <include
        layout="@layout/activity_line"
        />
    <include layout="@layout/community_activityitme_head"
        android:layout_width="match_parent"
        android:id="@+id/titile_rela"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/padding_width_small"
        android:layout_marginRight="@dimen/padding_width_small"
        android:layout_marginTop="@dimen/padding_width_small"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/content_font_size"
        android:textColor="@color/community_content_gray"
        android:id="@+id/content_smalllink"
        android:layout_marginLeft="@dimen/padding_width_small"
        android:layout_marginRight="@dimen/padding_width_small"
        android:layout_marginTop="@dimen/padding_width_small"
        android:gravity="center_vertical"
        android:includeFontPadding="false"
        android:lineSpacingMultiplier="1.2"
        />
    <include layout="@layout/community_parenting_item_sp"
        android:layout_width="match_parent"
        android:id="@+id/parenting_item_rela"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/padding_width_small"
        android:layout_marginRight="@dimen/padding_width_small"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="@dimen/padding_width_small"
        />

    <include
        layout="@layout/activity_line"
        />
    <include layout="@layout/activity_driver" />
</LinearLayout>
